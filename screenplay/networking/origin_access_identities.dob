resources:
  origin_access_identities:
    - name: my-cloudfront-oai                // A descriptive name for the Origin Access Identity (OAI)
      comment: Used for S3 access from CloudFront // A comment describing the purpose of the OAI
      tags:                                  // Tags associated with the OAI
        - Key: Environment                   // Key for the tag (e.g., "Environment")
          Value: Production                  // Value for the tag (e.g., "Production")
        - Key: Project                       // Key for the tag (e.g., "Project")
          Value: MediaDelivery               // Value for the tag (e.g., "MediaDelivery")
